package iptu_vv;

public class Imovel {

    private int matricula;
    private String nome;
    private float impostos;
    private int mesesAtraso;

    public void cadastroImovel(int matricula, String nome, float imposto, int mesesAtraso) {
        this.matricula = matricula;
        this.nome = nome;
        this.mesesAtraso = mesesAtraso;
        this.impostos = imposto;

        System.out.print("Matr√≠cula: " + this.matricula +
                "\nNome: " + this.nome +
                "\nMeses de atraso: " + this.mesesAtraso +
                "\nImpostos: " + this.impostos + "\n");
    }

    public int getMatricula() {
        return matricula;
    }

    public String getNome() {
        return nome;
    }

    public float getImpostos() {
        return impostos;
    }

    public int getMesesAtraso() {
        return mesesAtraso;
    }
}
